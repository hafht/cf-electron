#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/haht/Documents/creativeforce/test/cf-electron/node_modules/.pnpm/@electron+windows-sign@1.2.2/node_modules/@electron/windows-sign/bin/node_modules:/Users/haht/Documents/creativeforce/test/cf-electron/node_modules/.pnpm/@electron+windows-sign@1.2.2/node_modules/@electron/windows-sign/node_modules:/Users/haht/Documents/creativeforce/test/cf-electron/node_modules/.pnpm/@electron+windows-sign@1.2.2/node_modules/@electron/node_modules:/Users/haht/Documents/creativeforce/test/cf-electron/node_modules/.pnpm/@electron+windows-sign@1.2.2/node_modules:/Users/haht/Documents/creativeforce/test/cf-electron/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/haht/Documents/creativeforce/test/cf-electron/node_modules/.pnpm/@electron+windows-sign@1.2.2/node_modules/@electron/windows-sign/bin/node_modules:/Users/haht/Documents/creativeforce/test/cf-electron/node_modules/.pnpm/@electron+windows-sign@1.2.2/node_modules/@electron/windows-sign/node_modules:/Users/haht/Documents/creativeforce/test/cf-electron/node_modules/.pnpm/@electron+windows-sign@1.2.2/node_modules/@electron/node_modules:/Users/haht/Documents/creativeforce/test/cf-electron/node_modules/.pnpm/@electron+windows-sign@1.2.2/node_modules:/Users/haht/Documents/creativeforce/test/cf-electron/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../@electron/windows-sign/bin/electron-windows-sign.js" "$@"
else
  exec node  "$basedir/../../../@electron/windows-sign/bin/electron-windows-sign.js" "$@"
fi
