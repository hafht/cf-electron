#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/haht/Documents/Workspace/cf-electron/node_modules/.pnpm/electron-vite@5.0.0_vite@7.3.0_@types+node@25.0.2_/node_modules/electron-vite/bin/node_modules:/Users/haht/Documents/Workspace/cf-electron/node_modules/.pnpm/electron-vite@5.0.0_vite@7.3.0_@types+node@25.0.2_/node_modules/electron-vite/node_modules:/Users/haht/Documents/Workspace/cf-electron/node_modules/.pnpm/electron-vite@5.0.0_vite@7.3.0_@types+node@25.0.2_/node_modules:/Users/haht/Documents/Workspace/cf-electron/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/haht/Documents/Workspace/cf-electron/node_modules/.pnpm/electron-vite@5.0.0_vite@7.3.0_@types+node@25.0.2_/node_modules/electron-vite/bin/node_modules:/Users/haht/Documents/Workspace/cf-electron/node_modules/.pnpm/electron-vite@5.0.0_vite@7.3.0_@types+node@25.0.2_/node_modules/electron-vite/node_modules:/Users/haht/Documents/Workspace/cf-electron/node_modules/.pnpm/electron-vite@5.0.0_vite@7.3.0_@types+node@25.0.2_/node_modules:/Users/haht/Documents/Workspace/cf-electron/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/electron-vite.js" "$@"
else
  exec node  "$basedir/../../bin/electron-vite.js" "$@"
fi
